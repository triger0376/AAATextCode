{
  "name": "@ali/builder-debug-utils",
  "version": "3.1.6",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "offline",
    "readonly"
  ],
  "scripts": {
    "build": "tnpm run clean && tsc -p ./tsconfig.json && tnpm run build:dist",
    "build:dist": "rollup -c rollup.config.js",
    "clean": "rm -rf ./dist ./lib",
    "watch": "tsc-watch -p ./tsconfig.json --onSuccess 'tnpm run build:dist'",
    "lint": "tslint --project ./tsconfig.json",
    "lint:fix": "tslint --fix --project ./tsconfig.json",
    "prepublishOnly": "tnpm run build",
    "generate": "node scripts/generator.js",
    "generate:watch": "node scripts/generator-watch.js",
    "test": "jest --coverage",
    "ci": "tnpm run lint && tnpm run build && tnpm run test"
  },
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  },
  "tnpm": {
    "mode": "yarn",
    "lockfile": "enable"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@types/bent": "^7.0.2",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^13.11.1",
    "chokidar": "^3.5.3",
    "jest": "^26.6.3",
    "rollup": "^2.10.3",
    "rollup-plugin-terser": "^5.3.0",
    "ts-jest": "^26.5.6",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bent": "^7.3.1",
    "fs-extra": "^9.0.0"
  }
}
